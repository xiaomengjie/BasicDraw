<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="XfermodeView">
        <attr name="xfermode" format="string"/>
    </declare-styleable>

    <declare-styleable name="MultiLineView">
        <attr name="imagePaddingTop" format="dimension"/>
        <attr name="imagePaddingEnd" format="dimension"/>
        <attr name="imageSize" format="dimension"/>
        <attr name="imageSrc" format="reference"/>
    </declare-styleable>

    <declare-styleable name="UnderlineView">
        <attr name="type" format="enum">
            <enum name="path" value="0" />
            <enum name="region" value="1" />
        </attr>
        <attr name="family" format="string" />
        <attr name="text" format="string" />
    </declare-styleable>
</resources>